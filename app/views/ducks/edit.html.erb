<ul>
    <% @duck.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>



<h1> Edit Duck: </h1>

<%= form_for(@duck) do |f| %>
<%= f.label :name  %>
<%= f.text_field :name  %><br><br>
<%= f.label :description  %>
<%= f.text_field :description  %><br><br>
<%= f.label :student_id  %>
<%= f.collection_select :student_id, Student.all, :id, :name %><br><br>
<%= f.submit %>
<% end  %>